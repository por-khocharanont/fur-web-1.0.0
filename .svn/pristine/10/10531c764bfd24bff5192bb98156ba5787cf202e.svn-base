<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org"
	xmlns:layout="http://www.ultraq.net.nz/thymeleaf/layout"
	layout:decorate="~{template/layout}">
<head>
<title>Report</title>
<script type="text/javascript" th:src="@{/res/js/views/report/report.application.js}" > 
</script>
</head>
<body>
	<section layout:fragment="content" class="bg-shadow">
		<div class="row">
			<div class="col-md-12">
				<div class="navigation navi-left">
					<ul>
						<img th:src="@{/res/img/icon/user_request_inactive.png}" height="42" width="42">
						<B th:utext="#{report.label.title}"></B>
						<li>
							<a th:href="@{/report}" th:utext="#{report.hnavbar.application}" class ="link-active"></a>
						</li>
						<li>
							<a th:href="@{/report/eligible}" th:utext="#{report.hnavbar.eligible}"></a>
						</li>
						<li>
							<a th:href="@{/report/ur}" th:utext="#{report.hnavbar.searchur}"></a>
						</li>
					</ul>
					<br>
				</div>
				<div class="col-md-12">
					<h1 class="font-label-title" th:utext="#{report.label.title}"></h1>
					<hr class="hr-line">
					<form id="formReportApp" method="POST" class="form-horizontal" th:action="@{/report/serach/applist}" >
						<div class="col-md-1"></div>
						<div class="col-md-10">
							<div class="form-group">
								<label class="col-md-3 col-md-offset-1 control-label">Username</label>
								<div class="col-md-5">
									<input class="form-control" name="username" maxlength="100"></input>
								</div>
							</div>
							<div class="form-group">
								<label class="col-md-3 col-md-offset-1 control-label">Application</label>
								<div class="col-md-5">
									<input class="form-control" name="appName" maxlength="100"></input>
								</div>
							</div>
							<div class="form-group">
								<label class="col-md-3 col-md-offset-1 control-label">Type</label>
								<div class="col-md-5">
									<select class="form-control" name ="type">
										<option value="">All</option>
										<option value="1">Application</option>
										<option value="2">Telecom</option>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-md-3 col-md-offset-1 control-label">Authen By</label>
								<div class="col-md-5">
									<select class="form-control" name ="authenticationType">
										<option value="">All</option>
										<option value="1">USB</option>
										<option value="2">OTP</option>
										<option value="3">Username/Password</option>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-md-3 col-md-offset-1 control-label">Status</label>
								<div class="col-md-5">
									<select class="form-control" name ="status">
										<option value="">All</option>
										<option value="1">Active</option>
										<option value="2">Inactive</option>
									</select>
								</div>
							</div>
							<div class="form-group">
								<label class="col-md-3 col-md-offset-1 control-label">Period</label>
								<div class="col-md-5">
											<label class="radio-inline"><input type="radio" name="period" value="" checked>All</label>
											<label class="radio-inline"><input type="radio" name="period" value="1">Permanent</label>
											<label class="radio-inline"><input type="radio" name="period" value="2">Temporary</label>
								</div>
							</div>
							<div class="form-group">
								<label class="col-md-3 col-md-offset-1 control-label"></label>
								<div class="col-md-7" >
									<div class="userDatepicker col-md-12" style="display:none;">
											<input type="text" id="dteStart" style="width: 30%;" name="startDate" readonly="true" /> 
											<span class=" detail-head "> To <input style="width: 30%;" type="text" id="dteTo" name="endDate" readonly='true' /></span>
									</div>
								</div>
							</div>
							<div class="form-group">
								<div class="btn-panel col-md-12 text-center">
									<button type="submit" class="btn btn-green btn-lg-w1" id="searchBtn">Search</button>
									<button type="reset" class="btn btn-grey btn-lg-w1">Reset</button>
								</div>
							</div>	
						</div>
						<div class="col-md-1"></div>
						
						<div class="col-md-12 ">
							<div class="pull-right">
								<button type="button" class="btn btn-green btn-lg-w1 empty" id="export">Export</button>
							</div>
						</div>
						<div class="form-group">
							<div class="col-md-12 data-table tableReportApplication" >
										<table id="tableReportApplication"></table>
										<div id="tableReportApplicationPager"></div>
							</div>
							<div class="col-md-12 text-center empty" th:utext="|#{report.label.empty}|">
							</div>
						</div>
					</form>
					<form id="exportReportData" method="POST" class="form-horizontal" th:action="@{/report/export/applicationDefault}" >
						<input name="jsonData" type="hidden" />
					</form>
					<input name="urlWatchlist" type="hidden" th:action="@{/userrequest/}"/>
				</div>
			</div>
		</div>
	</section>
	<footer class="footer hide" layout:fragment="footer">
		<div class="container">
			<p class="text-center">footer</p>
		</div>
	</footer>
</body>
</html>
